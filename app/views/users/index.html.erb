<div class="container">
  <div class="row">
    <h3>Alumnos</h3>
    <div class="column">
      <table class="table">
        <tbody>
        
        <tr>
          <th><%= sortable "first_name" %></th>
          <th><%= sortable "last_name" %></th>
          <th><%= sortable "contact_number" %></th>
        </tr>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
